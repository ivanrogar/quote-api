{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "beberlei/doctrineextensions": "^1.3",
        "cocur/slugify": "^4.0",
        "composer/package-versions-deprecated": "1.11.99.1",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.3",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.8",
        "friendsofphp/php-cs-fixer": "^2.18",
        "gedmo/doctrine-extensions": "^3.0",
        "phpdocumentor/reflection-docblock": "^5.2",
        "phpmd/phpmd": "^2.9",
        "predis/predis": "^1.1",
        "squizlabs/php_codesniffer": "^3.5",
        "stof/doctrine-extensions-bundle": "^1.6",
        "symfony/amqp-messenger": "5.2.*",
        "symfony/apache-pack": "^1.0",
        "symfony/console": "5.2.*",
        "symfony/dotenv": "5.2.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "5.2.*",
        "symfony/messenger": "5.2.*",
        "symfony/monolog-bundle": "^3.6",
        "symfony/property-access": "5.2.*",
        "symfony/property-info": "5.2.*",
        "symfony/proxy-manager-bridge": "5.2.*",
        "symfony/security-bundle": "5.2.*",
        "symfony/serializer": "5.2.*",
        "symfony/yaml": "5.2.*",
        "webmozart/assert": "^1.10"
    },
    "require-dev": {
        "behat/behat": "^3.8",
        "behatch/contexts": "^3.3",
        "friends-of-behat/mink-browserkit-driver": "^1.5",
        "friends-of-behat/symfony-extension": "^2.2",
        "symfony/phpunit-bridge": "^5.2"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "behat": [
            "bin/console c:c --env=test",
            "bin/console d:d:c --env=test --if-not-exists",
            "bin/console d:s:d -f --env=test",
            "bin/console d:s:u -f --env=test",
            "bin/console app:install --env=test",
            "APP_ENV=test behat --colors"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.2.*"
        }
    },
    "name": "ivanrogar/quoteapi",
    "description": ""
}
